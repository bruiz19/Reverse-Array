#include <iostream>

using namespace std;

int main(){
int myarray[10];
cout << "Enter the array size: " << endl;
int size;
cin >> size;
cout << "Enter the numbers for the array: " << endl;

for(int i = 0; i < size; ++i){
cin >> myarray[i];
}

for(int i = size - 1; i >=0; --i){
cout << myarray[i];
}
return 0;

}
